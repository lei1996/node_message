    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "core-js": "^3.6.5",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "socket.io-client": "^2.3.0"
  }
